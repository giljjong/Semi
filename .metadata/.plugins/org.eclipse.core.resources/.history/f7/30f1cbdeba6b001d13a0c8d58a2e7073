package com.gdu.semi.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.ui.Model;

import com.gdu.semi.domain.SleepUserDTO;
import com.gdu.semi.domain.UserDTO;
import com.gdu.semi.mapper.AdminMapper;

@Service
public class AdminServiceImpl implements AdminService {
	
	@Autowired
	private AdminMapper adminMapper;
	
	@Override
	public List<UserDTO> findAllUsers(Model model) {
		return adminMapper.selectUserList();
	}
	
	@Override
	public List<SleepUserDTO> findAllSleepUsers() {
		return adminMapper.selectSleepUserList();
	}
	
}
